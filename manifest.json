{
  "manifest_version": 3,
  "name": "System Info Extension",
  "version": "1.0",
  "permissions": [
    "system.cpu",
    "system.memory",
    "system.storage",
    "runtime",
    "management",
    "enterprise.deviceAttributes",
    "storage",
    "identity"

  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html"
  },

  "key": ""
}
